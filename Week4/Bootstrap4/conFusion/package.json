{
  "name": "confusion",
  "version": "1.0.0",
  "description": "This is a website for Ristorante Con Fusion",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "./node_modules/lite-server/bin/lite-server",
    "scss": "./node_modules/node-sass/bin/node-sass -o css/ css/",
    "watch:scss": "./node_modules/onchange/cli.js 'css/*.scss' -- npm run scss",
    "watch:all": "./node_modules/parallelshell/index.js 'npm run watch:scss' 'npm run lite'",
    "clean": "./node_modules/rimraf/bin.js dist",
    "copyfonts": "./node_modules/copyfiles/copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imagemin": "./node_modules/imagemin-cli/cli.js img/* -o dist/img",
    "usemin": "./node_modules/usemin-cli/bin/usemin contactus.html -d dist --htmlmin -o dist/contactus.html && ./node_modules/usemin-cli/bin/usemin aboutus.html -d dist --htmlmin -o dist/aboutus.html && ./node_modules/usemin-cli/bin/usemin index.html -d dist --htmlmin -o dist/index.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "author": "gvgramazio",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^1.2.0",
    "cssmin": "^0.4.3",
    "grunt": "^1.0.1",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-sass": "^2.0.0",
    "htmlmin": "0.0.7",
    "imagemin-cli": "^3.0.0",
    "imagemin-gifsicle": "^5.2.0",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.3.0",
    "node-sass": "^4.5.3",
    "onchange": "^3.2.1",
    "parallelshell": "^3.0.2",
    "rimraf": "^2.6.2",
    "time-grunt": "^1.4.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "font-awesome": "^4.7.0"
  }
}
